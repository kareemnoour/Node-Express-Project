{
    "version":2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "PORT": 3000,
      "DATA_BASE_URL": "mongodb://localhost:27017/mydatabase"
    },
    "public": {
      "ignore": ["**/*.ejs"]
    }
  }